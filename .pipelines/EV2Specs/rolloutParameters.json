{
    "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/rolloutParameters.json",
    "contentVersion": "1.0.0.0",
    "shellExtensions": [
        {
            "name": "PushImage",
            "type": "PushImageShellType",
            "properties": {
                "maxExecutionTime": "PT10M"
            },
            "package": {
                "reference": {
                    "path": "drop_build_restore/Run.tar" 
                }
            },
            "launch": {
                "command": [
                    "/bin/bash",
                    "/package/unarchive/Run.sh"
                ],
                "environmentVariables": [
                    {
                        "name": "PROD_CONTAINER_REGISTRY",
                        "value": "__ACR_NAME__"
                    },
                    {
                        "name": "IMAGE_NAMESPACE",
                        "value": "__IMAGE_NAME_SPACE__"
                    },
                    {
                        "name": "TARDIRECTORY",
                        "reference": {
                            "path": "__IMAGE_TAR_DIRECTORY__/multi_arch_image.tar"
                        }
                    }
                ],
                "identity": {
                    "type": "userAssigned",
                    "userAssignedIdentities": [
                        "__ACRPUSH_MANAGED_IDENTITY_ARM_ID__"
                    ]
                }
            }
        }
    ]
}